User		"<%= @pound_user %>"
Group		"<%= @pound_group %>"
LogLevel	<%= @log_level %>
Control 	"<%= @control_socket %>"

ListenHTTP
	Address <%= @listen_http_address %>
	Port	<%= @listen_http_port %>
	Service <% @backend_servers.each do |ip, port| %>
